<footer id="footer">
  <div class="footer-left">

		<a class="icon" target="_blank" rel="noopener" href="/atom.xml" aria-label="RSS">
			<i class="fas fa-rss"></i>
		</a>

		<a class="icon" target="_blank" rel="noopener" href="https://www.reddit.com/r/YesNoDebate" aria-label="Reddit">
			<i class="fab fa-reddit"></i>
		</a>

		<a class="icon" target="_blank" rel="noopener" href="https://twitter.com/YesNoDebate" aria-label="Twitter">
			<i class="fab fa-twitter"></i>
		</a>

		<a class="icon" target="_blank" rel="noopener" href="mailto:info@yesnodebate.org" aria-label="E-Mail">
			<i class="fas fa-envelope"></i>
		</a>

    <!--
    <%= __('footer.copyright') %> &copy;
    <% var endYear = (theme.copyright && theme.copyright.end_year) ? theme.copyright.end_year : new Date().getFullYear() %>
    <% var startYear = (theme.copyright && theme.copyright.start_year) ? theme.copyright.start_year : new Date().getFullYear() %>
    <%= startYear >= endYear ? endYear : startYear + "-" + endYear %>
    <%= config.author || config.title %>
    -->
  </div>
  <div class="footer-right">
    <nav>
      <ul>
        <% for (var i in theme.nav2) { %> 
          <li><a href="<%- url_for(theme.nav2[i]) %>"><%= __('nav2.'+i).replace("nav2.", "") %></a></li>
        <% } %>
      </ul>
    </nav>
  </div>
</footer>
